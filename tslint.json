{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "interface-name": false,
        "max-classes-per-file": false,
        "no-empty": false,
        "trailing-comma": false,
        "arrow-parens": false,
        "no-console": false,
        "quotemark": [
            true,
            "single"
        ]
    },
    "rulesDirectory": []
}
